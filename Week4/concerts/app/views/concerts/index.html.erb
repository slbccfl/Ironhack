<h1>All Concerts</h1>
 
<table>
  <tr>
    <th>Artist</th>
    <th>Venue</th>
    <th>City</th>
    <th>When</th>
    <th>Price</th>
  </tr>
 
  <% @upcoming_concerts.each do |concert| %>
    <tr>
      <td><%= link_to concert.artist, concert_path(concert) %></td>
      <td><%= concert.venue %></td>
      <td><%= concert.city %></td>
      <td><%= concert.datetime %></td>   
      <td><%= concert.price %></td>   
    </tr>
  <% end %>
</table>